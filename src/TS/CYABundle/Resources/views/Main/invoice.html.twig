        <td class="text-center" style="background-color: #1e68b4; color: white;" colspan="4"><b>GASTOS ADICIONALES
                (Sujeto a cambio)</b></td>
    </tr>
    {% if quotation.discretionarySpending %}
        <tr>
            <td class="text-left">Valor de VISA</td>
            <td class="text-left">{{ quotation.discretionarySpending.descriptionVisa }}</td>
            <td class="text-right">{{ quotation.discretionarySpending.coinVisa.code  | default('') }}</td>
            <td class="text-right">
                <span class="">
                    <strong>
                        {{ quotation.discretionarySpending.valueVisa | number_format(2, '.', ',') }}
                    </strong>
                </span>
            </td>
        </tr>
        <tr>
            <td class="text-left">Valor adicional</td>
            <td class="text-left">{{ quotation.discretionarySpending.descriptionAdicional }}</td>
            <td class="text-right">{{ quotation.discretionarySpending.coinAdicional.code | default('') }}</td>
            <td class="text-right">
                <span class="">
                    <strong>
                        {{ quotation.discretionarySpending.valueAdicional | number_format(2, '.', ',') }}
                    </strong>
                </span>
            </td>
        </tr>
        <tr>
            <td class="text-left">Valor tarjeta de asistencia</td>
            <td class="text-left">{{ quotation.discretionarySpending.descriptionShipping }}</td>
            <td class="text-right">{{ quotation.discretionarySpending.coinShipping.code  | default('') }}</td>
            <td class="text-right">
                <span class="">
                    <strong>
                        {{ quotation.discretionarySpending.valueShipping | number_format(2, '.', ',') }}
                    </strong>
                </span>
            </td>
        </tr>
        <tr>
            <td class="text-left" colspan="4" style="word-wrap: break-word;">
                {{ quotation.note }}
            </td>
