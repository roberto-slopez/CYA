    {% endif %}
    <tr>
        <td class="text-center page" style="background-color: #1e68b4; color: white;" colspan="4">OBSERVACIONES</td>
    </tr>
    <tr>
        <td  class="text-center" colspan="4">
            {{ agency.observations | raw }}
        </td>
    </tr>
    <tr>
        <td class="text-center" style="background-color: #1e68b4; color: white;" colspan="4">Avenida 19 No 103 - 75
            PBX 4842040 Bogot
        </td>
    </tr>
    <tr>
        <td class="text-center" colspan="4">http://www.kaplaninternational.com/co</td>
    </tr>
</table>
</div>
