    </style>
</head>
<body>
<div class="row block-section">
    <div class="col-md-12 text-center">
        <img height="50px"
             src="data:image/png;base64,"
             style="display: block;margin: 0 auto;">
    </div>
    <div class="col-md-12 text-center">
        {% if quotation.promocion %}
            <h2>Promocin valida hasta: {{ quotation.promocion.expiration |date("d/m/Y") }}</h2>
        {% endif %}
        {#<b>Nombre:</b>{{ quotation.client.fullName }} <br/>#}
        <span style="margin: 0; font-size: 20px; line-height: 32px; font-weight: bold">{{ quotation.client.fullName }}</span>
        <br/>
        <b>Destino</b>: {{ quotation.country.name }}/{{ quotation.headquarter.name }}
        <b>Fecha</b>: {{ "now"|date("d/m/Y") }}<br>
        {% if quotation.type == 'FLEXIBLE' %}
            <b>Tipo curso</b>: {{ quotation.course.name }} {#<b>Fecha inicio</b>: {inici}#}
        {% elseif quotation.type == 'PACKAGE' %}
            <b>Tipo paquete</b>: {{ quotation.package.name }} {#<b>Fecha inicio</b>: {inici}#}
        {% elseif quotation.type == 'EXAM' %}
            <b>Tipo examen</b>: {{ quotation.exam.name }} {#<b>Fecha inicio</b>: {inici}#}
        {% endif %}
        <b>
            Semanas de curso: </b>{{ quotation.semanas }}
        {% if quotation.semanasLodging %}
            | <b>Semanas de alojamiento: </b>{{ quotation.semanasLodging }}
        {% endif %}
        {% if quotation.startDate %}
            | <b>Fecha inicio</b> {{ quotation.startDate |date("d/m/Y") }}
        {% endif %}
        <br>
        <b>Asesora</b>: {{ quotation.seller.fullName }} | <i class="fa fa-phone"></i> {{ quotation.seller.telephone }} |
        <i class="fa fa-envelope-o"></i> {{ quotation.seller.userSeller.email }}
    </div>
</div>
<div style="font-size: 16px;">
    <table class="table table-vcenter" style="display: table;">
        <thead>
        <tr>
            <th class="text-center"></th>
            <th class="text-center">Descripcin</th>
            <th class="text-center">Moneda</th>
            <th class="text-center">Monto</th>
        </tr>
        </thead>
        <tbody>
        <tr>
            <td class="text-center" colspan="4" style="background-color: #1e68b4;color: white;">
                Detalles del curso Kaplan (costos en {{ quotation.country.coin.code }}).
            </td>
        </tr>
        {% if quotation.type == 'FLEXIBLE' %}
            <tr>
                <td class="text-left"><strong>Inscripcin</strong></td>
                <td class="text-left"><strong>{{ quotation.course.descriptionInscription }}</strong></td>
                <td class="text-right">{{ quotation.country.coin.code }}</td>
                <td class="text-right">
                <span>
                    <b>{{ quotation.course.priceInscription | number_format(2, '.', ',') }}</b>
                </span>
                </td>
            </tr>
            <tr>
                <td class="text-left"><strong>Curso</strong></td>
                <td class="text-left"><strong>{{ quotation.course.name }}</strong> {{ quotation.course.description }}
                </td>
                <td class="text-right">{{ quotation.country.coin.code }}</td>
                <td class="text-right">
                <span>
                    <b>{{ quotation.amountCourse | number_format(2, '.', ',') }}</b>
                </span>
                </td>
            </tr>
        {% elseif quotation.type == 'PACKAGE' %}
            <tr>
                <td class="text-left"><strong>Inscripcin</strong></td>
                <td class="text-left"><strong>{{ quotation.package.descriptionInscription }}</strong></td>
                <td class="text-right">{{ quotation.country.coin.code }}</td>
                <td class="text-right">
                <span>
                    <b>{{ quotation.package.priceInscription | number_format(2, '.', ',') }}</b>
                </span>
                </td>
            </tr>
            <tr>
                <td class="text-left"><strong>Paquete</strong></td>
                <td class="text-left">{{ quotation.package.name }}</td>
                <td class="text-right">{{ quotation.country.coin.code }}</td>
                <td class="text-right">
                <span>
                    <b>{{ quotation.amountCourse | number_format(2, '.', ',') }}</b>
                </span>
                </td>
            </tr>
        {% elseif quotation.type == 'EXAM' %}
            <tr>
                <td class="text-left"><strong>Inscripcin</strong></td>
                <td class="text-left"><strong>{{ quotation.exam.descriptionInscription }}</strong></td>
                <td class="text-right">{{ quotation.country.coin.code }}</td>
                <td class="text-right">
                <span>
                    <b>{{ quotation.exam.priceInscription | number_format(2, '.', ',') }}</b>
                </span>
                </td>
            </tr>
            <tr>
                <td class="text-left"><strong>Curso</strong></td>
                <td class="text-left"><strong>{{ quotation.exam.name }}</strong> {{ quotation.exam.description }}</td>
                <td class="text-right">{{ quotation.country.coin.code }}</td>
                <td class="text-right">
                <span>
                    <b>{{ quotation.amountCourse | number_format(2, '.', ',') }}</b>
                </span>
                </td>
            </tr>
        {% endif %}
        {% if quotation.lodgingpackage %}
            <tr>
                <td class="text-left"><strong>Alojamiento</strong></td>
                <td class="text-left">
                    <b>({{ quotation.lodgingpackage.name }}) </b> {{ quotation.lodgingpackage.description }}</td>
                <td class="text-right">{{ quotation.country.coin.code }}</td>
                <td class="text-right">
                <span>
                    <b>{{ quotation.amountLodging | number_format(2, '.', ',') }}</b>
                </span>
                </td>
            </tr>
        {% endif %}
        {% if quotation.lodging %}
            <tr>
                <td class="text-left"><strong>Alojamiento</strong></td>
                <td class="text-left">
                    <b>({{ quotation.lodging.name }})
                        {% if quotation.lodging.type == "DOBLE" or quotation.lodging.type == "MULTI" %}
                            COMPARTIDA {# << TODO: Aadir traduccion  #}
                        {% else %}
                            {{ quotation.lodging.type }}
                        {% endif %}
                    </b>
                    {{ quotation.lodging.description }}
                    {% if quotation.semanasLodging %}
                        <b>Semanas {{ quotation.semanasLodging }}</b>
                    {% endif %}
                </td>
                <td class="text-right">{{ quotation.country.coin.code }}</td>
                <td class="text-right">
                <span>
                    <b>{{ quotation.amountLodging | number_format(2, '.', ',') }}</b>
                </span>
                </td>
            </tr>
        {% endif %}
        {% for service in quotation.service %}
            <tr>
                <td class="text-left"><strong>{{ service.name }}</strong></td>
                {% set isSimpleService = true %}
                {% if service.isHealthCoverage %}

                    {% set isSimpleService = false %}
                    {% if service.useAmountInitialWeeks %}
                        {% set isSimpleService = false %}

                        {% if quotation.semanas >= service.initialWeeks %}
                            <td class="text-left">
                                <span class="">{{ service.price | number_format(2, '.', ',') }}</span>
                                {{ service.description }}
                                {#<strong> Meses: {{ (quotation.semanas / 4) + 1 }}</strong>#}
                            </td>
                            <td class="text-right">{{ quotation.country.coin.code }}</td>
                            <td class="text-right">
                                <span class=""><b>{{ (service.price * ((quotation.semanas / 4) +1)) | number_format(2, '.', ',') }}</b></span>
                            </td>
                        {% else %}
                            <td class="text-left">
                                <span class="">{{ service.price | number_format(2, '.', ',') }}</span>
                                {{ service.description }}
                                <strong> Meses: {{ (quotation.semanas / 4) +1 }}</strong>
                            </td>
                            <td class="text-right">{{ quotation.country.coin.code }}</td>
                            <td class="text-right">
                                <span class=""><b>{{ (service.price * 0) | number_format(2, '.', ',') }}</b></span>
                            </td>
                        {% endif %}
                    {% else %}
                        <td class="text-left">
                            <span class="">{{ service.price | number_format(2, '.', ',') }}</span>
                            {{ service.description }}
                            {#<strong> Meses: {{ (quotation.semanas / 4) +1  }}</strong>#}
                        </td>
                        <td class="text-right">{{ quotation.country.coin.code }}</td>
                        <td class="text-right">
                            <span class=""><b>{{ (service.price * ((quotation.semanas / 4) +1)) | number_format(2, '.', ',') }}</b></span>
                        </td>
                    {% endif %}

                {% elseif service.summerSupplement %}
                    {% set isSimpleService = false %}
                    <td class="text-left">
                        <span class="">{{ service.price | number_format(2, '.', ',') }}</span>
                        {{ service.description }}
                        <strong> Semanas: {{ quotation.summerSupplement }}</strong>
                    </td>
                    <td class="text-right">{{ quotation.country.coin.code }}</td>
                    <td class="text-right">
                        <span class=""><b>{{ (service.price * quotation.summerSupplement) | number_format(2, '.', ',') }}</b></span>
                    </td>
                {% elseif service.chargePerWeekCourse %}
                    {% set isSimpleService = false %}
                    {% if service.usesLimitWeeks %}

                        {% if quotation.semanas <= service.limitWeek %}
                            <td class="text-left">
                                <span class="">{{ service.price | number_format(2, '.', ',') }}</span>
                                {{ service.description }}
                                <strong> Semanas: {{ quotation.semanas }}</strong>
                            </td>
                            <td class="text-right">{{ quotation.country.coin.code }}</td>
                            <td class="text-right">
                                <span class=""><b>{{ (service.price * quotation.semanas)| number_format(2, '.', ',') }}</b></span>
                            </td>
                        {% else %}
                            <td class="text-left">
                                <span class="">{{ service.price | number_format(2, '.', ',') }}</span>
                                {{ service.description }}
                                <strong> Semanas: {{ service.limitWeek }}</strong>
                            </td>
                            <td class="text-right">{{ quotation.country.coin.code }}</td>
                            <td class="text-right">
                                <span class=""><b>{{ (service.price * service.limitWeek) | number_format(2, '.', ',') }}</b></span>
                            </td>
                        {% endif %}

                    {% else %}
                        <td class="text-left">
                            <span class="">{{ service.price | number_format(2, '.', ',') }}</span>
                            {{ service.description }}
                            <strong> Semanas: {{ quotation.semanas }}</strong>
                        </td>
                        <td class="text-right">{{ quotation.country.coin.code }}</td>
                        <td class="text-right">
                            <span class=""><b>{{ (service.price * quotation.semanas) | number_format(2, '.', ',') }}</b></span>
                        </td>
                    {% endif %}

                {% elseif service.chargePerWeekLodging %}
                    {% set isSimpleService = false %}
                    {% if service.usesLimitWeeks %}

                        {% if quotation.semanas <= service.limitWeek %}
                            <td class="text-left">
                                <span class="">{{ service.price | number_format(2, '.', ',') }}</span>
                                {{ service.description }}
                                <strong> Semanas: {{ quotation.semanasLodging }}</strong>
                            </td>
                            <td class="text-right">{{ quotation.country.coin.code }}</td>
                            <td class="text-right">
                                <span class=""><b>{{ (service.price * quotation.semanasLodging) | number_format(2, '.', ',') }}</b></span>
                            </td>
                        {% else %}
                            <td class="text-left">
                                <span class="">{{ service.price | number_format(2, '.', ',') }}</span>
                                {{ service.description }}
                                <strong> Semanas: {{ service.limitWeek }}</strong>
                            </td>
                            <td class="text-right">{{ quotation.country.coin.code }}</td>
                            <td class="text-right">
                                <span class=""><b>{{ (service.price * service.limitWeek) | number_format(2, '.', ',') }}</b></span>
                            </td>
                        {% endif %}

                    {% else %}
                        <td class="text-left">
                            <span class="">{{ service.price | number_format(2, '.', ',') }}</span>
                            {{ service.description }}
                            <strong> Semanas: {{ quotation.semanasLodging }}</strong>
                        </td>
                        <td class="text-right">{{ quotation.country.coin.code }}</td>
                        <td class="text-right">
                            <span class=""><b>{{ (service.price * quotation.semanasLodging) | number_format(2, '.', ',') }}</b></span>
                        </td>
                    {% endif %}

                {% elseif service.useAmountInitialWeeks %}
                    {% set isSimpleService = false %}
                    {% if quotation.semanas >= service.initialWeeks %}
                        <td class="text-left">
                            <span class="">{{ service.price | number_format(2, '.', ',') }}</span>
                            {{ service.description }}
                            <strong> Semanas: {{ quotation.semanas }}</strong>
                        </td>
                        <td class="text-right">{{ quotation.country.coin.code }}</td>
                        <td class="text-right">
                            <span class=""><b>{{ (service.price * quotation.semanas) | number_format(2, '.', ',') }}</b></span>
                        </td>
                    {% else %}
                        <td class="text-left">
                            <span class="">{{ service.price | number_format(2, '.', ',') }}</span>
                            {{ service.description }}
                            <strong> Semanas: 0</strong>
                        </td>
                        <td class="text-right">{{ quotation.country.coin.code }}</td>
                        <td class="text-right">
                            <span class=""><b>{{ (service.price * 0) | number_format(2, '.', ',') }}</b></span>
                        </td>
                    {% endif %}

                {% elseif service.multiplesOfFour %}
                    {% set isSimpleService = false %}
                    {% set keyValue = 0 %}
                    {% for key, i in range(0, 100, 4) if quotation.semanas >= i %}
                        {% set keyValue = key %}
                    {% endfor %}
                    <td class="text-left">
                        <span class="">{{ service.price | number_format(2, '.', ',') }}</span>
                        {{ service.description }}
                        <strong> {{ keyValue }}</strong>
                    </td>
                    <td class="text-right">{{ quotation.country.coin.code }}</td>
                    <td class="text-right">
                        <span class=""><b>{{ (service.price * keyValue) | number_format(2, '.', ',') }}</b></span>
                    </td>
                {% elseif isSimpleService %}
                    <td class="text-left">
                        {{ service.description }}
                    </td>
                    <td class="text-right">{{ quotation.country.coin.code }}</td>
                    <td class="text-right">
                        <span class=""><b>{{ service.price | number_format(2, '.', ',') }}</b></span>
                    </td>
                {% endif %}
            </tr>
        {% endfor %}
        {% for manualMultiplier in quotation.manualMultiplier %}
            <tr>
                <td class="text-left"><strong>{{ manualMultiplier.service.name }}</strong></td>
                <td class="text-left"> {{ manualMultiplier.service.description }}
                    <strong>Cantidad {{ manualMultiplier.quantity }}</strong></td>
                <td class="text-right">{{ quotation.country.coin.code }}</td>
                <td class="text-right"><span
                            class=""><b>{{ manualMultiplier.price | number_format(2, '.', ',') }}</b></span>
                </td>
            </tr>
        {% endfor %}
        <tr>
            <td class="text-left"><strong>Total</strong></td>
            <td class="text-left">Moneda local <strong>{{ quotation.country.coin.code }}</strong></td>
            <td class="text-right">{{ quotation.country.coin.code }}</td>
            <td class="text-right"><span
                        class=""><b> {{ quotation.totalLocal | number_format(2, '.', ',') }}</b></span>
            </td>
        </tr>
        <tr>
            <td class="text-left"><strong>Total en pesos</strong></td>
            <td class="text-left">
                Cambio <strong>{{ quotation.country.coin.code }}</strong> a <b>COP</b>
                <b>{{ quotation.country.coin.currentExChangeRate | number_format(4, '.', ',') }}</b> -
                Tasa hasta el <b>{{ quotation.country.coin.currentExChangeRateExpirate | date('d/m/Y') }}</b>
            </td>
            <td class="text-right"><b>COP</b></td>
            <td class="text-right"><span
                        class=""><b>{{ quotation.totalLocalCountry | number_format(2, '.', ',') }}</b></span>
            </td>
        </tr>
        </tbody>
    </table>
</div>
{#<div style="overflow: hidden!important; page-break-inside: avoid; !important;">#}
<table class="table table-vcenter" style="display: table; font-size: 16px;">
