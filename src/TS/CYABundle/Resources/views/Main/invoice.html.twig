        }

        .table thead > tr > th > small {
            font-weight: 400;
            font-size: 75%;
        }

        .table thead > tr > th,
        .table thead > tr > td,
        .table tfoot > tr > th,
        .table tfoot > tr > td {
            padding-top: 14px;
            padding-bottom: 14px;
        }

